{
    "name": "elcast",
    "version": "1.4.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "packageManager": "pnpm@10.6.5",
    "scripts": {
        "api:dev": "pnpm run --filter api start:dev",
        "api:studio": "pnpm run --filter api db:studio",
        "api:seed": "pnpm run --filter api db:seed",
        "web:dev": "pnpm run --filter web dev",
        "web:codegen": "pnpm run --filter web codegen",
        "clean-all": "cd apps/api/ && rm -rf node_modules dist && cd ../web && rm -rf node_modules .next"
    },
    "engines": {
        "node": "^20.9.0",
        "pnpm": ">=9.5.0"
    }
}
